<%- | Hash $config_options | -%>
#
# HEADER: This file is managed by Puppet
# HEADER: Do not make modifications directly!
#
###############################################################################
#
# /etc/default/useradd
#
<%- $config_options.keys.sort.each |$key| { -%>
<%- $value = $config_options[$key] -%>
<%- if $value =~ Variant[String[1], Integer] { -%>
<%= $key %> <%= $value %>
<%- } -%>
<%- } -%>
